{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug 'synthgen' (cwd=synthgen)",
      "program": "${cargo:program}",
      "cargo": {
        "args": [
          "build",
          "--manifest-path=${workspaceFolder}/backend/sudoku/Cargo.toml",
          "--bin=synthgen",
          "--package=synthgen"
        ],
        "filter": { "name": "synthgen", "kind": "bin" }
      },
      "args": [],
      "env": {},
      "cwd": "${workspaceFolder}/backend/sudoku/train/synthgen",
      "sourceLanguages": ["rust"]
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug single test (selected)",
      "cargo": {
        "args": [
          "test",
          "--no-run",
          "--manifest-path",
          "${workspaceFolder}/backend/sudoku/solver/Cargo.toml"
        ],
        "filter": {
          "kind": "lib"
        }
      },
      "program": "${cargo:program}",
      "cwd": "${workspaceFolder}/backend/sudoku/solver",
      "args": ["${selectedText}", "--nocapture"],
      "env": {
        "RUST_BACKTRACE": "1"
      }
    }
  ]
}
